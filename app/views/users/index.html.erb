<h1>ユーザ一覧画面</h1>
<table>
      <thead>
        <tr>
          <th>id</th>
          <th>プロフィール写真</th>
          <th>名前</th>
          <th>性別</th>
          <th>メールアドレス</th>
          <th>年齢</th>
          <th>一言紹介</th>
          <th>更新日</th>
          <th>作成日</th>
        </tr>
      </thead>
 
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= image_tag user.image.to_s, width:50 %></td>
            <td><%= user.name %></td>
            <td><%= user.gender %></td>
            <td><%= user.mail %></td>
            <td><%= user.age %></td>
            <td><%= user.introduction %></td>
            <td><%= user.updated_at %></td>
            <td><%= user.created_at %></td>
          </tr>
        <% end %>
      </tbody>
    </table>