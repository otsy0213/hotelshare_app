<h1>ログイン画面</h1>
<%= form_with url: login_path do |form| %> 
  <ul>
    <li>
      　<%= form.text_field :mail, placeholder: "Mail_address"%>
    </li>
    <li>
        <%= form.password_field :password, placeholder: "Password"%>
    </li>
    <li>
        <%= form.submit "ログインする"%>
    </li>
  </ul>
<% end %>